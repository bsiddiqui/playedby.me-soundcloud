<div class="well">
	<table class='table table-hover'>
		<tbody class ="list" style ="font-weight:bold;">


			<% @party_profile.each_with_index do |p, index| %>
			<tr>

				<td>
					<%= link_to "Join!", "/partyprofiles?party_profile_id=#{p.id}", :class => "btn btn-mini btn-primary"%>

				</td>

				<td style = "padding-right: 250px; text-align: left;">
					 <%= p.name %>
				</td>

				<td style = "width:90px; padding-right: 350px; text-align: center;">
					<%= p.date %>
				</td>

				<td style = "width:150px; padding-right: 100px;">
					<%= p.location %>
				</td>


			</tr>
			<% end %>
		</tbody>
	</table>
</div>