<div class="well">
	<table class='table table-hover'>
		<tbody class ="list" style ="font-weight:bold;">


			<% @party_profile.each_with_index do |p, index| %>
			<tr>

				<td style = "padding-left: 40px;">
					<%= "#{index+1}" %>
				</td>

				<td>
					<%= p.name %>
				</td>

				<td>
					<%= p.location %>
				</td>


				<td style = "width:250px; padding-left: 80px;">
					<%= p.date %>
				</td>
				<td>
					<%= link_to "Join!", "/partyprofiles?party_profile_id=#{p.id}" %>

				</td>

			</tr>
			<% end %>
		</tbody>
	</table>
</div>